pipeline {
 	  agent any
		stages{
		
		stage("Git Checkout"){
			steps{
			    git credentialsId: '7222e992-c2da-4457-9852-fb7b15b0d5ca', url: 'https://github.com/prakashbisen0/HelloJava.git'
			}
		      }
		
		stage("Meven Build"){
			steps {
				sh "mvn clean package"
			
			      }  
			    }
	          }
          
	}
